<Application
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Mooville.QUno.Universal"
    x:Class="Mooville.QUno.Universal.App"
    RequestedTheme="Light">
    <Application.Resources>
        <SolidColorBrush x:Key="CardRedBrush" Color="#FFED7D31"/>
        <SolidColorBrush x:Key="CardBlueBrush" Color="#FF5B9DB5"/>
        <SolidColorBrush x:Key="CardYellowBrush" Color="#FFFFC000"/>
        <SolidColorBrush x:Key="CardGreenBrush" Color="#FF70AD47"/>
        <SolidColorBrush x:Key="CardBlackBrush" Color="#FF303030"/>
        <SolidColorBrush x:Key="CardTransparentBrush" Color="Transparent"/>
        <x:Double x:Key="SmallWindowWidthBreakpoint">0</x:Double>
        <x:Double x:Key="SmallWindowHeightBreakpoint">0</x:Double>
        <x:Double x:Key="LargeWindowWidthBreakpoint">640</x:Double>
        <x:Double x:Key="LargeWindowHeightBreakpoint">480</x:Double>
        <x:Double x:Key="ExtraLargeWindowWidthBreakpoint">1024</x:Double>
    </Application.Resources>
</Application>
